<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/groupoptions.xsd">
    <import>
        <categories>
            <category name="admin">
                <showorder>3</showorder>
            </category>
                <category name="admin.content">
                    <parent>admin</parent>
                </category>
                    <!-- newslettersystem category -->
                    <category name="admin.content.newslettersystem">
                        <parent>admin.content</parent>
                        <options>module_newslettersystem</options>
                    </category>
                    <!-- /newslettersystem category -->
        </categories>
        <options>
            <!-- newslettersystem options -->
            <option name="admin.content.newslettersystem.canSeeSubscriberOverview">
                <categoryname>admin.content.newslettersystem</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <showorder>1</showorder>
            </option>
            <option name="admin.content.newslettersystem.canDeleteSubscribers">
                <categoryname>admin.content.newslettersystem</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <showorder>2</showorder>
            </option>
            <option name="admin.content.newslettersystem.canImportSubscriber">
                <categoryname>admin.content.newslettersystem</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <showorder>3</showorder>
            </option>
            <option name="admin.content.newslettersystem.canSeeNewsletterOverview">
                <categoryname>admin.content.newslettersystem</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <showorder>4</showorder>
            </option>
            <option name="admin.content.newslettersystem.canWriteNewsletter">
                <categoryname>admin.content.newslettersystem</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <showorder>5</showorder>
            </option>
            <option name="admin.content.newslettersystem.canEditNewsletter">
                <categoryname>admin.content.newslettersystem</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <showorder>6</showorder>
            </option>
            <option name="admin.content.newslettersystem.canSendNewsletter">
                <categoryname>admin.content.newslettersystem</categoryname>
                <optiontype>boolean</optiontype>
                <defaultvalue>0</defaultvalue>
                <showorder>7</showorder>
            </option>
            <!-- /newslettersystem options -->
        </options>
    </import>
</data>